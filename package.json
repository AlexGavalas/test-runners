{
    "name": "test-runners",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": "18"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "yarn ava && yarn uvu && yarn mocha && yarn jasmine && yarn jest && yarn vitest && yarn node:test && yarn xv && yarn unexpected && yarn tape && yarn tap && yarn japa",
        "ava": "ava ./tests/ava.tsx",
        "uvu": "node --no-warnings --loader=tsm -r global-jsdom/register ./tests/uvu.tsx",
        "mocha": "mocha ./tests/mocha.tsx",
        "jasmine": "NODE_OPTIONS=\"--no-warnings --loader=tsm\" jasmine --config=jasmine.json",
        "jest": "jest -- ./tests/jest.spec.tsx",
        "vitest": "vitest run ./tests/vitest.spec.tsx",
        "node:test": "node --no-warnings --loader=tsm -r global-jsdom/register ./tests/node.spec.tsx",
        "xv": "xv --no-warnings --loader=tsm --require=global-jsdom/register ./tests/xv.tsx",
        "unexpected": "mocha ./tests/unexpected.tsx",
        "tape": "node --no-warnings --loader=tsm -r global-jsdom/register ./tests/tape.tsx",
        "tap": "node --no-warnings --loader=tsm -r global-jsdom/register ./tests/tap.tsx",
        "japa": "node --no-warnings --loader=tsm -r global-jsdom/register ./japa.ts",
        "test:fastify": "vitest run server/fastify",
        "test:sinon": "mocha server/sinon.test.ts"
    },
    "dependencies": {
        "fastify": "4.17.0",
        "fastify-plugin": "4.5.0",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@japa/expect": "2.0.2",
        "@japa/runner": "2.5.1",
        "@japa/spec-reporter": "1.3.3",
        "@testing-library/react": "14.0.0",
        "@types/autocannon": "7.9.0",
        "@types/jasmine": "4.3.2",
        "@types/jest": "29.5.2",
        "@types/mocha": "10.0.1",
        "@types/node": "18.11.19",
        "@types/react": "18.2.8",
        "@types/react-dom": "18.2.4",
        "@types/sinon": "10.0.15",
        "@types/tap": "15.0.8",
        "@types/tape": "5.6.0",
        "@vitejs/plugin-react": "4.0.0",
        "@vitest/coverage-c8": "0.31.4",
        "autocannon": "7.11.0",
        "ava": "5.3.0",
        "c8": "7.14.0",
        "chai": "4.3.7",
        "global-jsdom": "9.0.1",
        "hdr-histogram-js": "3.0.0",
        "jasmine": "5.0.0",
        "jasmine-ts": "0.4.0",
        "jest": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "jsdom": "22.1.0",
        "mocha": "10.2.0",
        "msw": "1.2.1",
        "sinon": "15.1.0",
        "tap": "16.3.4",
        "tape": "5.6.3",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.1",
        "tsm": "2.3.0",
        "typescript": "5.1.3",
        "unexpected": "13.1.0",
        "uvu": "0.5.6",
        "vite": "4.3.9",
        "vitest": "0.31.4",
        "xv": "2.1.1"
    }
}
