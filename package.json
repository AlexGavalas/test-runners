{
    "name": "test-runners",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": "18"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "ava": "ava ./tests/ava.tsx",
        "uvu": "node  --no-warnings --loader tsm ./tests/uvu.tsx --tsmconfig tsm.config.js",
        "mocha": "mocha ./tests/mocha.tsx",
        "jasmine": "NODE_OPTIONS=\"--no-warnings --loader=ts-node/esm --experimental-specifier-resolution=node\" jasmine --config=jasmine.json",
        "jest": "jest -- ./tests/jest.spec.tsx",
        "vitest": "vitest run ./tests/vitest.spec.tsx",
        "node:test": "node --no-warnings --loader=ts-node/esm --experimental-specifier-resolution=node ./tests/node.spec.tsx"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@testing-library/react": "13.4.0",
        "@types/jasmine": "4.3.1",
        "@types/jest": "29.2.4",
        "@types/mocha": "10.0.1",
        "@types/node": "18.0.0",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.9",
        "@vitejs/plugin-react": "3.0.0",
        "@vitest/coverage-c8": "0.25.8",
        "ava": "5.1.0",
        "c8": "7.12.0",
        "global-jsdom": "8.6.0",
        "jasmine": "4.5.0",
        "jasmine-ts": "0.4.0",
        "jest": "29.3.1",
        "jest-environment-jsdom": "29.3.1",
        "jsdom": "20.0.3",
        "mocha": "10.2.0",
        "msw": "0.49.2",
        "ts-jest": "29.0.3",
        "ts-node": "10.9.1",
        "tsm": "2.3.0",
        "typescript": "4.9.4",
        "uvu": "0.5.6",
        "vite": "4.0.1",
        "vitest": "0.25.8"
    }
}
