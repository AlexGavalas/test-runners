{
    "name": "test-runners",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": "22"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "pnpm ava && pnpm uvu && pnpm mocha && pnpm jasmine && pnpm jest && pnpm vitest && pnpm node:test && pnpm xv && pnpm unexpected && pnpm tape && pnpm tap && pnpm japa",
        "ava": "ava ./tests/ava.tsx",
        "uvu": "node --no-warnings --import=tsx -r global-jsdom/register ./tests/uvu.tsx",
        "mocha": "mocha ./tests/mocha.tsx",
        "jasmine": "NODE_OPTIONS=\"--no-warnings --import=tsx\" jasmine --config=jasmine.json",
        "jest": "jest -- ./tests/jest.spec.tsx",
        "vitest": "vitest run ./tests/vitest.spec.tsx",
        "node:test": "node --no-warnings --import=tsx -r global-jsdom/register ./tests/node.spec.tsx",
        "xv": "xv --no-warnings --import=tsx --require=global-jsdom/register ./tests/xv.tsx",
        "unexpected": "mocha ./tests/unexpected.tsx",
        "tape": "node --no-warnings --import=tsx -r global-jsdom/register ./tests/tape.tsx",
        "tap": "node --no-warnings --import=tsx -r global-jsdom/register ./tests/tap.tsx",
        "japa": "node --no-warnings --import=tsx -r global-jsdom/register ./japa.ts",
        "test:fastify": "vitest run server/fastify",
        "test:sinon": "mocha server/sinon.test.ts"
    },
    "dependencies": {
        "fastify": "4.28.0",
        "fastify-plugin": "4.5.1",
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "devDependencies": {
        "@japa/expect": "3.0.2",
        "@japa/runner": "3.1.4",
        "@testing-library/react": "16.0.0",
        "@types/autocannon": "7.12.5",
        "@types/jasmine": "5.1.4",
        "@types/jest": "29.5.12",
        "@types/mocha": "10.0.6",
        "@types/node": "20.14.7",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@types/sinon": "17.0.3",
        "@types/tap": "15.0.11",
        "@types/tape": "5.6.4",
        "@vitejs/plugin-react": "4.3.1",
        "@vitest/coverage-c8": "0.33.0",
        "autocannon": "7.15.0",
        "ava": "6.1.3",
        "c8": "10.1.2",
        "chai": "5.1.1",
        "fast-check": "3.19.0",
        "global-jsdom": "9.2.0",
        "hdr-histogram-js": "3.0.0",
        "jasmine": "5.1.0",
        "jasmine-ts": "0.4.0",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "jsdom": "24.1.0",
        "mocha": "10.4.0",
        "msw": "2.3.1",
        "sinon": "18.0.0",
        "tap": "19.2.5",
        "tape": "5.8.1",
        "ts-jest": "29.1.5",
        "ts-node": "10.9.2",
        "tsx": "4.15.7",
        "typescript": "5.3.3",
        "undici": "6.19.2",
        "unexpected": "13.2.1",
        "uvu": "0.5.6",
        "vite": "5.3.1",
        "vitest": "1.6.0",
        "xv": "2.1.1"
    }
}
