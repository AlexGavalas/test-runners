{
    "name": "test-runners",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": "18"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "ava": "ava ./tests/ava.tsx",
        "uvu": "node --no-warnings --loader tsm -r global-jsdom/register ./tests/uvu.tsx --tsmconfig tsm.config.js",
        "mocha": "mocha --tsmconfig tsm.config.js ./tests/mocha.tsx",
        "jasmine": "NODE_OPTIONS=\"--no-warnings --loader=ts-node/esm --experimental-specifier-resolution=node\" jasmine --config=jasmine.json",
        "jest": "jest -- ./tests/jest.spec.tsx",
        "vitest": "vitest run ./tests/vitest.spec.tsx",
        "node:test": "node --no-warnings --loader=ts-node/esm --experimental-specifier-resolution=node -r global-jsdom/register ./tests/node.spec.tsx",
        "xv": "xv --no-warnings --loader=ts-node/esm --experimental-specifier-resolution=node --require=global-jsdom/register ./tests/xv.tsx",
        "unexpected": "mocha --tsmconfig tsm.config.js ./tests/unexpected.tsx"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@testing-library/react": "13.4.0",
        "@types/jasmine": "4.3.1",
        "@types/jest": "29.4.0",
        "@types/mocha": "10.0.1",
        "@types/node": "18.11.19",
        "@types/react": "18.0.27",
        "@types/react-dom": "18.0.10",
        "@vitejs/plugin-react": "3.1.0",
        "@vitest/coverage-c8": "0.28.4",
        "ava": "5.2.0",
        "c8": "7.12.0",
        "global-jsdom": "8.7.0",
        "jasmine": "4.5.0",
        "jasmine-ts": "0.4.0",
        "jest": "29.4.1",
        "jest-environment-jsdom": "29.4.1",
        "jsdom": "21.1.0",
        "mocha": "10.2.0",
        "msw": "1.0.0",
        "ts-jest": "29.0.5",
        "ts-node": "10.9.1",
        "tsm": "2.3.0",
        "typescript": "4.9.5",
        "unexpected": "13.1.0",
        "uvu": "0.5.6",
        "vite": "4.1.1",
        "vitest": "0.28.4",
        "xv": "2.1.1"
    }
}
